@using FormularzRejestracji.Models
@model FormularzRejestracji.Models.DaneRejestracji

@{
    ViewData["Title"] = "Potwierdzenie Rejestracji";
}

<h1>Rejestracja zakończona pomyślnie</h1>

<table border="1">
    <tr>
        <td>Imię:</td>
        <td>@Model.Imie</td>
    </tr>
    <tr>
        <td>Nazwisko:</td>
        <td>@Model.Nazwisko</td>
    </tr>
    <tr>
        <td>Email:</td>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <td>Telefon:</td>
        <td>@Model.Telefon</td>
    </tr>
    <tr>
        <td>Wiek:</td>
        <td>@Model.Wiek</td>
    </tr>
    <tr>
        <td>Miasto:</td>
        <td>@Model.Miasto</td>
    </tr>
</table>

<br>
<a href="@Url.Action("Formularz", "Rejestracja")">Wróć do formularza</a>
